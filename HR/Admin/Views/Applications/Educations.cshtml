@{
    List<Admin.Education> educations = ViewBag.educations ?? new List<Admin.Education>();
    
    string msg = ViewBag.msg;
    msg = (msg == null) ? "" : msg;
    
    ViewBag.Title = "Applications By Education Level";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="/Admin/Content/css/location.css" rel="stylesheet" type="text/css" />
<h3>Applications By Education Level</h3>
<div class="clearfix"></div>

<table>
    <thead>
        <tr>
            <th>Education Level</th>
            <th>Applications</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (Admin.Education ed in educations) {
            <tr>
                <td>@ed.edu_level</td>
                <td style="text-align: center;">@ed.applicationCount</td>
                <td class="last">
                    <a href="/Admin/Applications/Education/@ed.id" title="View @ed.edu_level applications">View Apps</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<script src="/Admin/Scripts/custom/location.js" type="text/javascript"></script>
